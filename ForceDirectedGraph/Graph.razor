<svg style="position:relative; left:0; top:0; width:100%; height:100%;" viewBox="@(MinPoint.X - 100) @(MinPoint.Y - 100) @(Width + 200) @(Height + 200)">
    <defs>
        <marker id='arrow-head' orient="auto"
                markerWidth='4' markerHeight='4'
                refX='0.1' refY='2'>
            <path d='M0,0 V4 L2,2 Z' fill="red" />
        </marker>
    </defs>
    @((MarkupString)String.Join("\n", EdgeElements.Select(e => e.OuterHtml)))
    @((MarkupString)String.Join("\n", NodeElements.Select(e => e.OuterHtml)))
</svg>
